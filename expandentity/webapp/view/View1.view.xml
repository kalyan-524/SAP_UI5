<mvc:View controllerName="ladera.expandentity.controller.View1"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m">
    <Page id="page" title="Order List">
    <Table items="{/Orders}">
        <columns>
            <Column>
            <Text text="Order Id"/>
            </Column>
             <Column>
            <Text text="Customer ID"/>
            </Column>
             <Column>
            <Text text="Ship Name"/>
            </Column>
             <Column>
            <Text text="Ship Country"/>
            </Column>
             <Column>
            <Text text="Shipped Date"/>
            </Column>
        </columns>
        <items>
        <ColumnListItem  type="Navigation" press="onorderListItemPress">
        <cells>
            <Text text="{OrderID}"/>
            <Text text="{CustomerID}"/>
            <Text text="{ShipName}"/>
            <Text text="{ShipCountry}"/>
            <!-- <Text text="{ShippedDate}"/> -->
            <Text text="{
                        path: 'ShippedDate',
                        type: 'sap.ui.model.type.Date',
                        formatOptions: {
                        pattern: 'dd-MM-yyyy'}
                        }"/>
        </cells>
        </ColumnListItem>
        </items>              
        </Table>
    </Page>
</mvc:View>